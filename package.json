{
  "name": "mayash-webapp",
  "version": "2.0.0",
  "description": "This is a project for Mayash website",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:server-only": "cross-env NODE_ENV='development' DEV_ENV='server' nodemon src/server/index.js --exec babel-node",
    "dev:server": "npm run build:client && NODE_ENV='development' DEV_ENV='server' nodemon src/server/index.js --exec babel-node",
    "dev:client-redux": "cross-env NODE_ENV='development' DEV_ENV='client' REDUX=true babel-node src/server/index.js",
    "dev:client": "cross-env NODE_ENV='development' DEV_ENV='client' babel-node src/server/index.js",
    "dev:client-build": "cross-env NODE_ENV='development' DEV_ENV='client' webpack --config=webpack/development.js",
    "dev": "cross-env NODE_ENV='development' DEV_ENV='client' nodemon src/server/index.js --exec babel-node",
    "eslint": "eslint src/server && eslint src/client",
    "eslint:fix": "eslint src/server --fix && eslint src/client --fix",
    "build:server": "cross-env NODE_ENV='production' babel src/server -d dist/server",
    "build:client-webpack": "cross-env NODE_ENV='production' webpack --config=webpack/production.js",
    "build:client-babel": "cross-env NODE_ENV='production' babel src/client -d dist/client",
    "build:client": "npm run build:client-babel && npm run build:client-webpack",
    "build": "npm run build:server && npm run build:client",
    "start": "cross-env NODE_ENV='production' node dist/server/index.js",
    "clear": "rm -rf dist",
    "deploy": "cross-env npm run build && gcloud app deploy app.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://source.developers.google.com/p/mayash-webapp/r/mayash-webapp"
  },
  "engines": {
    "node": "6.2.2",
    "npm": "3.10.5"
  },
  "author": "Himank Barve <hbarve1592@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bell": "8.4.0",
    "boom": "4.2.0",
    "cross-env": "3.1.4",
    "draft-js": "0.9.1",
    "good": "7.1.0",
    "good-console": "6.2.0",
    "good-file": "6.0.1",
    "good-http": "6.1.2",
    "good-squeeze": "5.0.1",
    "google-cloud": "0.45.2",
    "handlebars": "4.0.6",
    "hapi": "16.1.0",
    "hapi-auth-jwt2": "7.2.4",
    "hoek": "4.1.0",
    "inert": "4.1.0",
    "isomorphic-fetch": "2.2.1",
    "joi": "10.1.0",
    "jsonwebtoken": "7.2.1",
    "lodash": "4.17.4",
    "material-design-lite": "1.3.0",
    "medium-draft": "0.4.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "5.0.2",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.26.1",
    "eslint": "3.13.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "font-loader": "0.1.2",
    "html-webpack-plugin": "2.26.0",
    "node-sass": "4.2.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "remote-redux-devtools": "^0.5.7",
