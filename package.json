{
  "name": "mayash-webapp",
  "version": "2.0.0",
  "description": "This is a project for Mayash website",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:redux": "cross-env babel-node redux/index.js",
    "dev:build-webpack": "cross-env NODE_ENV='development' webpack --config=webpack/development.js",
    "dev:client": "npm run dev:build-webpack && cross-env NODE_ENV='development' babel-node webpack/devServer.js",
    "dev:server": "cross-env NODE_ENV='development' nodemon --watch src/server src/server/index.js --exec babel-node",
    "start:dev": "cross-env NODE_ENV='development' babel-node src/server/index.js",
    "eslint": "eslint src/server && eslint src/client",
    "eslint:fix": "eslint src/server --fix && eslint src/client --fix",
    "build:webpack": "cross-env NODE_ENV='production' webpack --config=webpack/production.js",
    "build:babel": "cross-env babel src -d dist",
    "build": "npm run build:babel && npm run build:webpack",
    "start": "cross-env NODE_ENV='production' node dist/server/index.js",
    "clean": "rm -rf dist/* && rm -rf public/*",
    "deploy": "cross-env npm run build && gcloud app deploy app.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://source.developers.google.com/p/mayash-webapp/r/mayash-webapp"
  },
  "engines": {
    "node": "6.9.5",
    "npm": "3.10.10"
  },
  "author": "Himank Barve <hbarve1592@gmail.com>",
  "license": "ISC",
