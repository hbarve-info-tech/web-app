{
  "name": "mayash-webapp",
  "version": "0.0.0",
  "description": "This is a project for Mayash website",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:server-only": "cross-env NODE_ENV='development' DEV_ENV='server' nodemon src/server/index.js --exec babel-node",
    "dev:server": "npm run build:client && NODE_ENV='development' DEV_ENV='server' nodemon src/server/index.js --exec babel-node",
    "dev:client-redux": "cross-env NODE_ENV='development' DEV_ENV='client' REDUX=true babel-node src/server/index.js",
    "dev:client": "cross-env NODE_ENV='development' DEV_ENV='client' babel-node src/server/index.js",
    "dev": "cross-env NODE_ENV='development' DEV_ENV='client' nodemon src/server/index.js --exec babel-node",
    "build:server": "cross-env NODE_ENV='production' babel src/server -d dist/server",
    "build:client": "cross-env NODE_ENV='production' webpack --config=webpack.config.pro.js",
    "build": "npm run build:server && npm run build:client",
    "start": "cross-env NODE_ENV='production' node dist/server/index.js",
    "clear": "rm -rf dist",
    "deploy": "cross-env npm run build && gcloud app deploy app.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://source.developers.google.com/p/mayash-webapp/r/mayash-webapp"
  },
  "engines": {
    "node": "6.2.2",
    "npm": "3.10.5"
  },
  "author": "Himank Barve <hbarve1592@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bell": "8.3.0",
    "boom": "4.2.0",
    "cross-env": "3.1.3",
    "crypto": "0.0.3",
    "draft-js": "0.9.1",
    "good": "7.0.2",
    "good-console": "6.3.1",
    "good-file": "6.0.1",
    "good-http": "6.1.1",
    "good-squeeze": "5.0.0",
    "google-cloud": "0.43.0",
    "hapi": "15.2.0",
    "hapi-auth-jwt2": "7.1.3",
    "inert": "4.0.2",
    "isomorphic-fetch": "2.2.1",
    "joi": "9.2.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.6",
    "medium-draft": "0.3.12",
    "path": "0.12.7",
    "react": "15.3.2",
    "react-bootstrap": "0.30.6",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-bootstrap": "0.23.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "font-loader": "0.1.2",
    "node-sass": "3.10.1",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.1.1",
    "sass-loader": "4.0.2",
