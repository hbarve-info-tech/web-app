{
  "name": "mayash-webapp",
  "version": "0.0.0",
  "description": "This is a project for Mayash website",
  "main": "dist/server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "cross-env npm run build:client && NODE_ENV='development' DEV_ENV='server' nodemon src/server/index.js --exec babel-node",
    "dev:client": "cross-env NODE_ENV='development' DEV_ENV='client' babel-node src/server/index.js",
    "dev": "cross-env NODE_ENV='development' DEV_ENV='client' nodemon src/server/index.js --exec babel-node",
    "build:server": "cross-env NODE_ENV='production' babel src/server -d dist/server",
    "build:client": "cross-env NODE_ENV='production' webpack",
    "build": "cross-env NODE_ENV='production' babel src -d dist && NODE_ENV='production' webpack",
    "start": "cross-env NODE_ENV='production' node dist/server/index.js",
    "clear": "rm -rf dist",
    "deploy": "cross-env npm run build && gcloud app deploy app.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://source.developers.google.com/p/mayash-webapp/r/mayash-webapp"
  },
  "engines": {
    "node": "6.2.2",
    "npm": "3.10.5"
  },
  "author": "Himank Barve <hbarve1592@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "boom": "^4.2.0",
    "cross-env": "^3.1.3",
    "font-awesome": "^4.6.3",
    "good": "^7.0.2",
    "good-console": "^6.3.1",
    "good-file": "^6.0.1",
    "good-http": "^6.1.1",
    "good-squeeze": "^5.0.0",
    "google-cloud": "^0.43.0",
    "hapi": "^15.2.0",
    "hapi-auth-jwt2": "^7.1.3",
    "inert": "^4.0.2",
    "path": "^0.12.7",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-preset-airbnb": "2.0.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "font-loader": "0.1.2",
    "node-sass": "3.10.0",
    "react-hot-loader": "3.0.0-beta.4",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  }
}
